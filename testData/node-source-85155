    val (targets, isRunOnly) =
      getStringList("runOnly") match {
        case seq if seq.nonEmpty =>
          (seq, true)
        case _ =>
          getStringList("runUntil") match {
            case seq if seq.nonEmpty =>
              (seq, false)
            case _ =>
              (persistedSteps.keys, false)
          }
      }
