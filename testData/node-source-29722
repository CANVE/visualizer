      val urlHandlerChain =
        if (fallbackUrlHandlers.isEmpty) {
          urlHandler
        } else {
          UrlToArtifact.chain(urlHandler, fallbackUrlHandlers.head, fallbackUrlHandlers.tail: _*)
        }
