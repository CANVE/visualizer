  implicit def convertToInputData[T, A <: FlatArtifact](p: PersistedProducer[T, A]): Producer[() => InputStream] = {
    p.copy(create =
      () => {
        p.get
        StreamIo.read(p.artifact.asInstanceOf[FlatArtifact])
      })
  }
