    val url = parsed.getScheme match {
      case null =>
        val fullPath = s"${rootUrl.getPath.reverse.dropWhile(_ == '/').reverse}/${parsed.getPath.dropWhile(_ == '/')}"
        new URI(
          rootUrl.getScheme,
          rootUrl.getHost,
          fullPath,
          rootUrl.getFragment
        )
      case _ => parsed
    }
